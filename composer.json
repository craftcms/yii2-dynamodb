{
  "name": "pixelandtonic/yii2-dynamodb",
  "description": "Yii2 implementation of a cache, session, and queue driver for DynamoDB",
  "license": "MIT",
  "authors": [
    {
      "name": "Jason McCallister",
      "email": "jason@pixelandtonic.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "yiisoft/yii2": "~2.0.14",
    "yiisoft/yii2-queue": "^2.1.0"
  },
  "require-dev": {
    "aws/aws-sdk-php": ">=2.4",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "pixelandtonic\\dynamodb\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests"
    }
  },
  "scripts": {
    "docker": "docker-compose up -d dynamodb",
    "test": "vendor/bin/phpunit --testdox",
    "tables": "make tables"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
